{% extends 'base.html.twig' %}

{% block title %}User list{% endblock %}

{% block body %}
    <h3 class="border-bottom border-gray pb-2 mb-0">List of application users</h3>
    {% for user in users %}
        {{ include('user/list-item.html.twig', { 'user': user }) }}
    {% endfor %}
    <div class="navigation">
        {{ knp_pagination_render(users) }}
    </div>
{% endblock %}
